<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jxd.student_information.mapper.ICourseMapper">
    <!--查询课程名称作为动态表头（cbb）-->
    <select id="selectAllCourseName" resultType="java.util.Map">
           SELECT course_name FROM course
           ORDER BY
           course_id
    </select>

</mapper>
